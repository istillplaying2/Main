		Tab.Name = name.. "Tab"
		Tab.Parent = Tab1
		Tab.Active = true
		Tab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Tab.BackgroundTransparency = 1.000
		Tab.BorderColor3 = Color3.fromRGB(30, 30, 30)
		Tab.Position = UDim2.new(0, 0, 0.049019821, 0)
		Tab.Size = UDim2.new(0, 406, 0, 319)
